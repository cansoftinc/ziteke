<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/widget192" android:layout_width="fill_parent" android:background="#FF8C00"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<TextView android:layout_height="30dp" android:id="@+id/rDetails"
			android:textSize="18sp" android:text="Welcome," android:textStyle="normal"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:layout_x="18dp" android:layout_y="4dp" android:layout_width="match_parent"></TextView>
		<TextView android:layout_height="30dp" android:id="@+id/rTitle"
			android:textSize="18sp" android:text="Kindly Select outlet to visit"
			android:textAppearance="?android:attr/textAppearanceSmall" android:textColor="#ffffff"
			android:layout_width="match_parent"></TextView>
		

		<TableLayout android:id="@+id/TableLayout_route"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:stretchColumns="*">
			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<TextView android:id="@+id/TextView01"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:layout_width="wrap_content" android:textStyle="bold" android:layout_height="wrap_content"
					android:textColor="#FFFFFF" android:text=""></TextView>
				
			</TableRow>
		</TableLayout>

		<TableLayout android:id="@+id/TableLayout_btnButton"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:stretchColumns="*">
			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<Button android:id="@+id/r_options" android:layout_width="77dp"
					android:textStyle="bold" android:layout_height="wrap_content"
					android:text="OPTIONS" android:layout_x="120dp" android:layout_y="412dp">
				</Button>
				<Button android:id="@+id/r_logout" android:layout_width="77dp"
					android:textStyle="bold" android:layout_height="wrap_content"
					android:text="LOGOUT" android:layout_x="210dp" android:layout_y="412dp">
				</Button>
			</TableRow>
		</TableLayout>

	</LinearLayout>
</ScrollView>
